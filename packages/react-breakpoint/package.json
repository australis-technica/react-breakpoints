{
  "name": "@australis/react-breakpoints",
  "version": "1.0.1-3",
  "description": "react redux media breakpoints",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "jest --env=jsdom",
    "build": "tsc -p . && npm run pack",
    "pack": "webpack-cli --config=./config/webpack.config.js --mode production"
  },
  "keywords": [
    "react",
    "redux",
    "breakpoint"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^9.6.4",
    "@types/react-redux": "^5.0.16",
    "flux-standard-action": "^2.0.1",
    "jest": "^22.4.3",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "ts-jest": "^22.4.2",
    "ts-loader": "^4.2.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-redux": ">=5.0.0",
    "redux": ">=3.0.0"
  },
  "directories": {
    "lib": "lib"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "__tests__/tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
